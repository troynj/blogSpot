<section class="authentication">
  <article class = "content">
  <h2>Sign In</h2>
  <form action="#">
    <label for="name">Name:</label>
    <input type="name" id="si_name" name="name" />
    <br /><br />
    <label for="password">Password:</label>
    <input type="password" id="si_password" name="password" />
    <br /><br />
    <button id="si">Sign In</button>
  </form>
</article>
<article class = "content">
  <h2>Sign Up</h2>
  <form action="#">
    <label for="name">Name:</label>
    <input type="name" id="su_name" name="name" />
    <br /><br />
    <label for="password">Password:</label>
    <input  id="su_password" name="password" />
    <br /><br />
    <label for="password-confirm">Confirm Password:</label>
    <input  id="su_password-confirm" name="password-confirm" />
    <br /><br />
    <button id="su" >Sign Up</button>
  </form>
</article>
</section>

  <script src="/script/signIn.js"></script>